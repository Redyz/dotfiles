URxvt*background:                     #08090C
URxvt*borderWidth:  1
URxvt*cursorColor:                    #d1d4e0
!URxvt*font: xft:Roboto Mono For Powerline:pixelsize=12,xft:Inconsolata\ for\ Powerline:pixelsize=12â‡§
URxvt*font: xft:GohuFont Powerline:style=Powerline:pixelsize=13
URxvt*foreground:                     #d1d4e0
URxvt*highlightColor:                 #1f222d
URxvt*highlightTextColor:             #C0C5CE
URxvt*intensityStyles:  true
URxvt*loginShell:       true
URxvt*matcher.button: 1
URxvt*perl-lib:   /usr/lib/urxvt/perl/
URxvt*scrollBar:  false
URxvt*scrollColor:                    #d1d4e0
URxvt*size-hints-honor: true
URxvt*termName:   rxvt-unicode-256color
URxvt*tripleclickwords: true
URxvt.depth: 		32
URxvt.letterSpace: -1
URxvt.matcher.pattern.1:	\\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
urxvt*background: black
urxvt*foreground: white
uxterm*background:	black
uxterm*foreground:	white
xterm*background:	black
xterm*foreground:	white

! Copy pasting
URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select,font-size
URxvt.url-select.launcher:  chromium
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
URxvt.keysym.C-S-Up:   font-size:incglobal
URxvt.keysym.C-S-Down: font-size:decglobal
